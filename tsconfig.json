{
  "compilerOptions": {
    "baseUrl": "./",
    "target": "esnext",
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "suppressImplicitAnyIndexErrors": true,
    "declaration": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "paths": {
      "@q25a25q/common": [
        "./packages/common/src/index.ts"
      ],
      "@q25a25q/gis": [
        "./packages/gis/src/index.ts"
      ],
      "@q25a25q/leaflet": [
        "./packages/leaflet/src/index.ts"
      ],
    }
  },
  "include": [
    "./tests/setupTests.ts",
    ".dumi/**/*",
    "jest.config.ts",
    ".dumirc.ts",
    "typings.d.ts",
    "**/src/**/*",
    "**/docs/**/*",
    "scripts/**/*",
    "**/demos",
    ".eslintrc.js",
    "tests",
    "jest.config.ts",
    "**/fixtures",
    "./tests/no-duplicated.ts",
    "packages/descriptions/src/demos/.tsx"
  ]
}